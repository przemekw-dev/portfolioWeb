{
  "compilerOptions": {
    "ignoreDeprecations": "6.0",
    "baseUrl": "src/",
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["components/*"],
      "@context/*": ["context/*"],
      "@utils/*": ["utils/*"],
      "@icons/*": ["icons/*"],
      "@graphql/*": ["graphql/*"],
      "@public/*": ["../public/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "src/components/FrutigerComponents.jsx"
  ],
  "exclude": ["node_modules"]
}
